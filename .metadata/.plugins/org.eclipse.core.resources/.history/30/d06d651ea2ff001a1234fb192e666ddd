import java.util.Scanner;

public class Contador {
	private String temperatura;
	private Scanner ingresar;
	String nombreUno="uno";
	String nombreDos="dos";
	String nombreTres="tres";
	String nombreCuatro="cuatro";
	String nombreCinco="cinco";
	String nombreSeis="seis";
	String nombreSiete="siete";
	String nombreOcho="ocho";
	String nombreNueve="nueve";
	String nombreCero="cero";
	
	public void ingresarDatos() {
		ingresar=new Scanner(System.in);
		System.out.println("Ingresar temperatura primer digito");
		temperatura=ingresar.next();
		
	}
	public void mostrar() {
		if(temperatura.equalsIgnoreCase(nombreUno) || temperatura.equalsIgnoreCase(nombreCero) || temperatura.equalsIgnoreCase(nombreTres)
		|| temperatura.equalsIgnoreCase(nombreDos)	|| temperatura.equalsIgnoreCase(nombreCuatro) || temperatura.equalsIgnoreCase(nombreCinco)
		|| temperatura.equalsIgnoreCase(nombreSeis) || temperatura.equalsIgnoreCase(nombreSiete) || temperatura.equalsIgnoreCase(nombreOcho)
		|| temperatura.equalsIgnoreCase(nombreNueve)){
			System.out.println(temperatura);
		}
	}
	
	public static void main(String[] args) {
		Contador conteo=new Contador();
		conteo.ingresarDatos();
		conteo.mostrar();
	}

}
